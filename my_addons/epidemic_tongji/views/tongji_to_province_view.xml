<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="tongji_to_province_tree_view" model="ir.ui.view">
        <field name="name">中国疫情分省统计详情</field>
        <field name="model">tongji.to.province</field>
        <field name="arch" type="xml">
            <tree>
                <field name="state_id"/>
                <field name="state_code" string="简称"/>
                <field name="total_confirm"/>
            </tree>
        </field>
    </record>

    <record id="tongji_to_province_act_window" model="ir.actions.act_window">
        <field name="name">中国疫情分省统计详情</field>
        <field name="res_model">tongji.to.province</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>